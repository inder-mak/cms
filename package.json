{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
